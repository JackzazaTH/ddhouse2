{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/og.jpg",
      "dest": "/og.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}